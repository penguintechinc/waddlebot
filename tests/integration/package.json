{
  "name": "waddlebot-integration-tests",
  "version": "1.0.0",
  "description": "Integration tests for WaddleBot multi-service workflows",
  "scripts": {
    "test": "jest --runInBand --verbose --testTimeout=30000",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.js"
    ],
    "testTimeout": 30000
  },
  "dependencies": {
    "jest": "^29.7.0",
    "axios": "^1.6.0",
    "ws": "^8.14.0",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3"
  }
}
